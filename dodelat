      let nameprompt = prompt ("введите число");
      let numberpromtp = [];
      let firstnumber = 1;
      let summ = [];
      console.log(nameprompt);
      for(let j = 0; j < nameprompt.length; j++){
        if(nameprompt[j] % 2 != 0){
            summ = summ + nameprompt[j];
        }
      }
      
      alert(`сумма всех нечетных чисел равна ${summ}`)
